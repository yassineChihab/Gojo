<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav_graph"
    app:startDestination="@id/espaceSushiFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.majjane.chefmajjane.AccueilFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_accueil" >
        <action
            android:id="@+id/action_accueilFragment_to_espaceSushiFragment"
            app:destination="@id/espaceSushiFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_homeFragment_to_profileMenuFragment"
            app:destination="@id/profileMenuFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/espaceSushiFragment"
        android:name="com.majjane.chefmajjane.views.FoodListFragment"
        android:label="fragment_espace_sushi"
        tools:layout="@layout/fragment_espace_sushi">
        <action
            android:id="@+id/action_espaceSushiFragment_to_profileMenuFragment"
            app:destination="@id/profileMenuFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_espaceSushiFragment_to_closeFragment"
            app:destination="@id/closeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_espaceSushiFragment_to_sushiDetailsFragment"
            app:destination="@id/sushiDetailsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <argument
            android:name="category_bundle"
            app:argType="com.majjane.chefmajjane.responses.AccueilResponseItem"
            />
        <action
            android:id="@+id/action_espaceSushiFragment_to_mesCommandesFragment"
            app:destination="@id/mesCommandesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_espaceSushiFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>

    </fragment>
    <fragment
        android:id="@+id/sushiDetailsFragment"
        android:name="com.majjane.chefmajjane.FoodDetailsFragment"
        android:label="fragment_sushi_details"
        tools:layout="@layout/fragment_sushi_details" >
        <argument
            android:name="article_bundle"
            app:argType="com.majjane.chefmajjane.responses.Article" />
        <action
            android:id="@+id/action_sushiDetailsFragment_to_espaceSushiFragment"
            app:destination="@id/espaceSushiFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/mesCommandesFragment"
        android:name="com.majjane.chefmajjane.views.MesCommandesFragment"
        android:label="fragment_mes_commandes"
        tools:layout="@layout/fragment_mes_commandes" >
        <argument
            android:name="article_list_bundle"
            app:argType="com.majjane.chefmajjane.responses.Article[]" />
        <action
            android:id="@+id/action_mesCommandesFragment_to_mapsFragment"
            app:destination="@id/mapsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_mesCommandesFragment_to_displayCommandsFragment"
            app:destination="@id/displayCommandsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_mesCommandesFragment_to_espaceSushiFragment"
            app:destination="@id/espaceSushiFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_mesCommandesFragment_to_confirmFragment"
            app:destination="@id/fragmentconfirmCommande"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_mesCommandesFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentconfirmCommande"
        android:name="com.majjane.chefmajjane.views.CommandeConfirm"
        android:label="fragment_commande_confirm"
        tools:layout="@layout/fragment_commande_confirm">
        <action
            android:id="@+id/action_voirCommande_to_fragmentProgress"
            app:destination="@id/fragmentProgress"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_confirmCommande_to_espaceSushiFragment"
            app:destination="@+id/espaceSushiFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>


    </fragment>
    <fragment
        android:id="@+id/fragmentProgress"
        android:name="com.majjane.chefmajjane.views.CommandProgress"
        android:label="fragment_progress"
        tools:layout="@layout/fragment_command_progress">
        <action
            android:id="@+id/action_ProgressCommande_to_espaceSushiFragment"
            app:destination="@+id/espaceSushiFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>


    </fragment>
    <fragment
        android:id="@+id/fragmentRepete"
        android:name="com.majjane.chefmajjane.views.RepeteCommadne"
        android:label="fragment_repete"
        tools:layout="@layout/fragment_repete_commadne">


        <action

            android:id="@+id/action_RepeteCommande_to_ConfirmFragment"
            app:destination="@+id/mesCommandesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            ></action>
        <action

            android:id="@+id/action_RepeteCommande_to_mesCommande"
            app:destination="@+id/VosCommandeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            ></action>

    </fragment>
    <fragment
        android:id="@+id/languageFragment"
        android:name="com.majjane.chefmajjane.views.LanguageFragment"
        android:label="fragment_language"
        tools:layout="@layout/fragment_language" />
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.majjane.chefmajjane.views.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" >
        <action
            android:id="@+id/action_ContactFragment_to_profilMenuFragment"
            app:destination="@id/profileMenuFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>

    </fragment>
    <fragment
        android:id="@+id/myProfileFragment"
        android:name="com.majjane.chefmajjane.views.MyProfileFragment"
        android:label="fragment_my_profile"
        tools:layout="@layout/fragment_my_profile">
        <action
            android:id="@+id/action_myProfileFragment_to_profilMenuFragment"
            app:destination="@id/profileMenuFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_myProfileFragment_to_changePasswordFragment"
            app:destination="@id/changePasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/changePasswordFragment"
        android:name="com.majjane.chefmajjane.ChangePasswordFragment"
        android:label="fragment_change_password"
        tools:layout="@layout/fragment_change_password" >
        <action
            android:id="@+id/action_changePasswordFragment_to_homeFragment"
            app:destination="@id/profileMenuFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.majjane.chefmajjane.views.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" >
        <action
            android:id="@+id/action_mapsFragment_to_mesCommandesFragment"
            app:destination="@id/mesCommandesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/displayCommandsFragment"
        android:name="com.majjane.chefmajjane.views.DisplayCommandsFragment"
        android:label="fragment_display_commands"
        tools:layout="@layout/fragment_display_commands" />
    <fragment
        android:id="@+id/reclamationFragment"
        android:name="com.majjane.chefmajjane.views.ReclamationFragment"
        android:label="fragment_reclamation"
        tools:layout="@layout/fragment_reclamation" >
        <action
            android:id="@+id/action_reclamationFragment_to_homeFragment"
            app:destination="@id/profileMenuFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/VosCommandeFragment"
        android:name="com.majjane.chefmajjane.views.VosCommandes"
        android:label="fragment_vos_commande"
        tools:layout="@layout/fragment_vos_commandes" >
        <action
            android:id="@+id/action_vosCommandeFragment_to_homeFragment"
            app:destination="@id/profileMenuFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_vosCommandeFragment_to_ProgressFragement"
            app:destination="@id/fragmentProgress"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_vosCommandeFragment_RepeteFragment"
            app:destination="@id/fragmentRepete"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>


    </fragment>
    <fragment
        android:id="@+id/profileMenuFragment"
        android:name="com.majjane.chefmajjane.views.ProfileMenuFragment"
        android:label="fragment_profile_menu"
        tools:layout="@layout/fragment_profile_menu" >
        <action
            android:id="@+id/action_profileMenuFragment_to_languageFragment"
            app:destination="@id/languageFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_profileMenuFragment_to_sushiFragment"
            app:destination="@id/espaceSushiFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_left"/>
        <action
            android:id="@+id/action_profileMenuFragment_to_vosCommandesFragment"
            app:destination="@+id/VosCommandeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_in_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_profileMenuFragment_to_contactFragment"
            app:destination="@id/contactFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_profileMenuFragment_to_reclamationFragment"
            app:destination="@id/reclamationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_profileMenuFragment_to_myProfileFragment"
            app:destination="@id/myProfileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
    </fragment>

    <fragment
        android:id="@+id/closeFragment"
        android:name="com.majjane.chefmajjane.views.ClosedFragement"
        tools:layout="@layout/fragment_closed_fragement"
        android:label="close_fragment"
        >

    </fragment>
</navigation>
